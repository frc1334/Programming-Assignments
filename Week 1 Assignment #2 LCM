#include <iostream>
#include <cmath>

using namespace std;

int main()
{
   int a=0;
   int b=0;
   int gcd;
   int lcm;
   cout << "Number A: ";
   cin >> a;
   cout << "Number B: ";
   cin >> b;
   for (int i=1; i<a && i<b; i++)
   {
       if(a%i==0 && b%i==0)
       {
           gcd=i;
       }

    }

    cout << "GCD: " << gcd << endl;


    lcm = abs(a*b)/gcd;

    cout << "LCM: " << lcm;


    return 0;
}
